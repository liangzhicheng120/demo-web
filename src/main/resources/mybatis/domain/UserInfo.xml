<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"   
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xinrui.demo.dao.UserInfoDao">
	<select id="getPasswordByName" resultType="UserInfo" parameterType="UserInfo">
		SELECT
		password
		FROM
		user_info
		WHERE name = #{name}
	</select>
</mapper>